<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update">
	<class name="model.vehicle.Vehicle" table="VEHICLES">

		<id column="id" name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>

		<!-- <property name="category" column="category" unique="false"
			type="model.vehicleType.Category" not-null="false" lazy="false" />
		-->
		<property name="description" type="string" column="DESCRIPTION" />
			
		<property name="passengerCapacity" type="integer" column="PASSAGER_CAPACITY" />

	</class>
</hibernate-mapping>